<h1>Enjob！</h1>
<div class="user_show">
  ユーザーの情報もろもろ
</div>
<div class="task_new">
  <%= link_to "Taskを作成する", new_task_path %>
</div>
<div class="task_index">
  <% @tasks.each do |task| %>
    <%= task.text %>
    〆切
    <%= task.deadline_at %>
    重要度
    <%= task.importance_i18n %>
    <%= link_to "EDIT", edit_task_path(task) %>
    <%= link_to "DELETE", task_path(task), method: :delete %>
  <% end %>
</div>